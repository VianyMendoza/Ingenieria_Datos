Algoritmo operadores
	num1 <- 100
	num2 <- 50
	
	resta <- num1 - num2
	Escribir "El resultado de la resta es: "
	Escribir  resta
	multiplicacion <- num1 *num2
	Escribir "El resultado de la multiplicacion es: "
	Escribir  multiplicacion
	menor_que <- num1 < num2
	Escribir "El resultado menor que es: "
	Escribir  menor_que
	mayor_que <- num1 > num2
	Escribir "El resultado mayor que es: "
	Escribir  mayor_que
	
FinAlgoritmo
